{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "libs/polyfill.min.js",
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://www.ptt.cc/bbs/*"
      ],
      "js": [
        "execute.js"
      ]
    }
  ]
}
